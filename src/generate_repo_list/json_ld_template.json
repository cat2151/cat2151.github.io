{
  "@context": "https://schema.org",
  "@type": "ProfilePage",
  "mainEntity": {
    "@type": "Person",
    "name": "{username}",
    "url": "https://{username}.github.io/",
    "sameAs": "https://github.com/{username}",
    "description": "Programmer and developer. Publishing {total} open source projects.",
    "image": "https://{username}.github.io/assets/favicon-512x512.png"
  },
  "description": "{og_description}",
  "url": "https://{username}.github.io/",
  "name": "{username}'s GitHub Repository List"
}
